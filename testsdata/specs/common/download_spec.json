{
  "files": [
    {
      "aql": {
        "items.find": {
          "repo": "jfrog-cli-tests-repo",
          "$or": [
            {
              "$and": [
                {
                  "path": {
                    "$match": "."
                  },
                  "name": {
                    "$match": "a1.in"
                  }
                }
              ]
            },
            {
              "$and": [
                {
                  "path": {
                    "$match": "*"
                  },
                  "name": {
                    "$match": "a1.in"
                  }
                }
              ]
            }
          ]
        }
      },
      "target": "out/download/aql/"
    },
    {
      "aql": {
        "items.find": {
          "repo": "jfrog-cli-tests-repo",
          "$or": [
            {
              "$and": [
                {
                  "path": {
                    "$match": "."
                  },
                  "name": {
                    "$match": "*"
                  }
                }
              ]
            },
            {
              "$and": [
                {
                  "path": {
                    "$match": "*"
                  },
                  "name": {
                    "$match": "*"
                  }
                }
              ]
            }
          ]
        }
      },
      "target": "out/download/aql_flat/",
      "flat": "true"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/a/*",
      "target": "out/download/flat_recursive/",
      "flat": "true",
      "recursive": "true"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/a/*3*",
      "target": "out/download/3_only_flat_recursive/",
      "flat": "true",
      "recursive": "true"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/a/*",
      "target": "out/download/flat_nonrecursive/",
      "flat": "true",
      "recursive": "false"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/a/*",
      "target": "out/download/nonflat_recursive/",
      "flat": "false",
      "recursive": "true"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/a/*",
      "target": "out/download/nonflat_nonrecursive/",
      "flat": "false",
      "recursive": "false"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/a/*",
      "target": "out/",
      "flat": "true",
      "recursive": "false"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/a/a1.in",
      "target": "out/download/simple/",
      "flat": "true",
      "recursive": "false"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/a/a1.in",
      "target": "out/download/rename/a1.out",
      "flat": "true",
      "recursive": "false"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/(a)/a1.in",
      "target": "out/download/simple_placeholder/{1}/",
      "flat": "true",
      "recursive": "false"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/(*).in",
      "target": "out/download/pattern_placeholder/{1}/",
      "flat": "true",
      "recursive": "false"
    },
    {
      "pattern": "jfrog-cli-tests-repo/downloadTestResources/*",
      "target": "out/download/properties/",
